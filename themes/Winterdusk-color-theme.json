  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#706c6c"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
